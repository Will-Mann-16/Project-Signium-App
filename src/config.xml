<?xml version='1.0' encoding='utf-8'?>
<widget id="com.willmann.wcsis" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>WCSIS</name>
    <description>
        Signing In App.
    </description>
    <author email="william@mannclan.net" >
        Will Mann
    </author>
    <feature name="http://api.phonegap.com/1.0/network"/>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <preference name="android-minSdkVersion" value="14" />
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="BackupWebStorage" value="none" />
    </platform>
    <preference name="DisallowOverscroll" value="true" />
    <preference name="orientaition" value="portrait" />
    <plugin name="cordova-plugin-whitelist" spec="~1.2.0" />
    <plugin name="cordova-plugin-console" spec="~1.0.1" />
    <plugin name="cordova-plugin-statusbar" spec="~1.0.1" />
    <plugin name="cordova-plugin-splashscreen" />
    <plugin name="cordova-plugin-background-mode" />
    <plugin name="cordova-plugin-device" />
    <plugin name="cordova-plugin-dialogs" />
    <splash src="assets/images/splash/default_splash.png" />
</widget>
